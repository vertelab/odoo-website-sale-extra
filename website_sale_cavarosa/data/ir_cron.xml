<odoo>
    <data noupdate="1">
        <record forcecreate="True" id="ir_cron_campaign_clear_cart_action" model="ir.cron">
            <field name="name">Expired UTM Campaign: Clear Cart Trigger</field>
            <field name="model_id" ref="model_utm_campaign"/>
            <field name="state">code</field>
            <field name="code">model._clean_expired_cart_campaign()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
            <field eval="False" name="active"/>
        </record>
    </data>
</odoo>